	<queryString>
		<![CDATA[SELECT
date,
    bmch_AGE_SCAN,
    BMTN_lecteur,
    BMTR_age,
    BMTR_CPT,
    BMTR_NSER,
    BMTR_MNT+0 BMTR_MNT,
    BMCH_NSER,
    concat(BMCH_ZBK,BMCH_LOC,BMCH_CPT,BMCH_RIB) as cpt_tire,
    BMCH_MNT+0 BMCH_MNT,
      (SELECT p.Lib FROM  r_ncrm_extraction.te_pacapt_sgma p where bmtr_AGE_SCAN=p.cdeAge and type="AG" group by 1) libAgeScann, concat(SUBSTRING(BMCH_CODE_ERREURS,1,4),"|",SUBSTRING(BMCH_CODE_ERREURS,5,8),"|",SUBSTRING(BMCH_CODE_ERREURS,9,12)) BMCH_CODE_ERREURS,
    BMCH_SQCA

FROM
    r_ncrm_extraction.tmp_tbl_eta_chq_sgma

Where BMCH_CONF='0'
 and BMCH_ope_eta<>'D'
 and BMtr_ope_eta<>'D'

and STR_TO_DATE(date, '%Y-%m-%d')=$P{dateInstance}
order by BMTN_lecteur]]>
	</queryString>
