	<queryString>
		<![CDATA[SELECT
    bmlcn_AGE_SCAN,
    BMTN_lecteur,
    BMTR_age,
    BMTR_CPT,
    BMTR_NSER,
    BMTR_MNT+0 BMTR_MNT,
    bmlcn_NSER,
    concat(bmlcn_ZBK,bmlcn_LOC,bmlcn_CPT,bmlcn_RIB) as cpt_tire,
    bmlcn_MNT+0 bmlcn_MNT,
    bmlcn_CODE_ERREURS,bmlcn_SQCA,
     bmlcn_dteec
FROM
    sgma_editions.tmp_tbl_eta_lcn_sgma
Where 1=1
 and bmlcn_ope_eta<>'D'
 and BMtr_ope_eta<>'D'

 and STR_TO_DATE(date, '%Y-%m-%d')=$P{dateInstance}
 and STR_TO_DATE(bmlcn_dteec, '%d-%m-%Y')<=$P{dtepre}
 and bmtn_lecteur=$P{cdepointCapture}
 and bmlcn_FLAG_SCAN_GR=$P{flag_scan_gr}
order by bmtr_age,bmlcn_dteec]]>
