	<queryString>
		<![CDATA[SELECT
date,
    bmlcn_AGE_SCAN,
    BMTN_lecteur,
    BMTR_age,
    BMTR_CPT,
    BMTR_NSER,
    BMTR_MNT+0 BMTR_MNT,
    bmlcn_NSER,
    concat(bmlcn_ZBK,bmlcn_LOC,bmlcn_CPT,bmlcn_RIB) as cpt_tire,
    bmlcn_MNT+0 bmlcn_MNT,
    bmlcn_CODE_ERREURS,bmlcn_SQCA,
 (SELECT p.Lib FROM  r_ncrm_extraction.te_pcapt p where bmlcn_AGE_SCAN=p.cdeAge and type="AG" and cdeBq="022" group by 1) libAgeScann,

CASE bmtr_mode

        WHEN '1' THEN 'Encaissement'
        WHEN '2' THEN 'Escompte'
        WHEN '3' THEN 'Risque'
        ELSE '---'
	END  AS  'bmtr_mode' ,
     STR_TO_DATE(bmlcn_dteec, '%d%m%Y') bmlcn_dteec
FROM
    r_ncrm_extraction.tmp_tbl_eta_lcn_sgma
Where 1=1
 and bmlcn_ope_eta<>'D'
 and BMtr_ope_eta<>'D'
 and (bmlcn_CONF='1'  or (bmlcn_CONF='0' and bmlcn_CODE_ERREURS='ABOP'))
and( bmlcn_REJET_DELTA<>'1' or bmlcn_REJET_DELTA is null )


 and STR_TO_DATE(date, '%Y-%m-%d')=$P{dateInstance}
 and STR_TO_DATE(bmlcn_dteec, '%d%m%Y')>$P{dtepre}


order by BMTN_lecteur,BMTR_NSER,STR_TO_DATE(bmlcn_dteec, '%d%m%Y') asc]]>
	</queryString>
