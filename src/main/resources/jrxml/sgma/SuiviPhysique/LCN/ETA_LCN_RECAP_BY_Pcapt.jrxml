	<queryString>
		<![CDATA[SELECT
    DATE AS DATE,
    lecteur LECTEUR,
	circuit circuit,
     LECTEUR_LIB AS LECTEUR_LIB,
    SUM(NBR_LCN_DEC) AS NBR_LCN_DEC,
    SUM(NBR_LCN_TRAIT) AS NBR_LCN_TRT,
    SUM(NBR_LCN_SUPP) AS NBR_LCN_SUPP,
    SUM(NBR_LCN_VF) AS NBR_LCN_VF,
    SUM(NBR_LCN_RD) AS NBR_LCN_RD,
    SUM(NBR_LCN_IMP) AS NBR_LCN_IMP
FROM
    (
    (-- REM & LCN Déclarer
    SELECT
	 v.date AS DATE,
             v.BMTN_lecteur AS LECTEUR,
             p.Lib AS LECTEUR_LIB,
			 p.circuit circuit,
             COUNT(*) AS NBR_LCN_DEC,
             0 AS NBR_LCN_TRAIT,
             0 AS NBR_LCN_SUPP,
             0 AS NBR_LCN_VF,
             0 AS NBR_LCN_RD,
             0 AS NBR_LCN_IMP
    FROM
        r_ncrm_extraction.tmp_tbl_suivi_LCN_sgma v,
        r_ncrm_extraction.te_pcapt p
    WHERE
        1 = 1
and p.cdeBq="022"
           and v.BMTN_lecteur=p.cdePcapt
           and dateReception is null
            and concat(date,v.BMTN_lecteur)in ($P!{listparams})


    GROUP BY date , lecteur
    ORDER BY date , lecteur)
    union
    (
    -- REM & LCN Traitées
    SELECT
	  v.date AS DATE,
            v.BMTN_lecteur AS LECTEUR,
            p.Lib AS LECTEUR_LIB,
			p.circuit circuit,
            0 AS NBR_LCN_DEC,
            COUNT(*) AS NBR_LCN_TRAIT,
            0 AS NBR_LCN_SUPP,
			0 AS NBR_LCN_VF,
			0 AS NBR_LCN_RD,
            0 AS NBR_LCN_IMP

    FROM
        r_ncrm_extraction.tmp_tbl_suivi_LCN_sgma v,
        r_ncrm_extraction.te_pcapt p
    WHERE
        1 = 1 and p.cdeBq="022"
            AND bmlcn_ope_eta <> 'D'
            AND BMTR_ope_eta <> 'D'
            and  (bmlcn_CONF = 1 or (bmlcn_CONF = 0 and bmlcn_CODE_ERREURS='ABOP'))

 and  v.bmlcn_FLAG_IMP is null  and  (v.bmlcn_REJET_DELTA<>'1' or v.bmlcn_REJET_DELTA is null)
            and v.BMTN_lecteur=p.cdePcapt
			and dateReception is null
          and concat(date,v.BMTN_lecteur)in ($P!{listparams})


    GROUP BY date , lecteur
    ORDER BY date , lecteur)
 UNION (
    -- REM & LCN ANO
    SELECT
        v.date AS DATE,
            v.BMTN_lecteur AS LECTEUR,
             p.Lib AS LECTEUR_LIB,
             p.circuit circuit,
			 0 AS NBR_LCN_DEC,
			 0 AS NBR_LCN_TRAIT,
			 count(*) AS NBR_LCN_SUPP,
			 0 AS NBR_LCN_VF,
			 0 AS NBR_LCN_RD,
             0 AS NBR_LCN_IMP
    FROM
        r_ncrm_extraction.tmp_tbl_suivi_LCN_sgma v,
        r_ncrm_extraction.te_pcapt p
    WHERE
        1 = 1 and p.cdeBq="022"
           AND (bmlcn_ope_eta ='D' )
		and v.BMTN_lecteur=p.cdePcapt
		and dateReception is null
    and concat(date,v.BMTN_lecteur)in ($P!{listparams})


        GROUP BY date , lecteur
    ORDER BY date , lecteur)
    UNION (
    -- REM & LCN VF
    SELECT
        v.date AS DATE,
             v.BMTN_lecteur AS LECTEUR,
             p.Lib AS LECTEUR_LIB,
             p.circuit circuit,
			 0 AS NBR_LCN_DEC,
			 0 AS NBR_LCN_TRAIT,
			 0 AS NBR_LCN_SUPP,
			 COUNT(*) AS NBR_LCN_VF,
			 0 AS NBR_LCN_RD,
             0 AS NBR_LCN_IMP
    FROM
        r_ncrm_extraction.tmp_tbl_suivi_LCN_sgma v,
        r_ncrm_extraction.te_pcapt p
    WHERE
        1 = 1 and p.cdeBq="022"
           AND (bmlcn_CONF <> 1  and bmlcn_CODE_ERREURS<>'ABOP')
		and v.BMTN_lecteur=p.cdePcapt
		and dateReception is null
     and concat(date,v.BMTN_lecteur)in ($P!{listparams})


        GROUP BY date , lecteur
    ORDER BY date , lecteur)
      UNION (
    -- REM & LCN RDELTA
    SELECT
        v.date AS DATE,
            v.BMTN_lecteur AS LECTEUR,
             p.Lib AS LECTEUR_LIB,
			  p.circuit circuit,
            0 AS NBR_LCN_DEC,
            0 AS NBR_LCN_TRAIT,
             0 AS NBR_LCN_SUPP,
            0 AS NBR_LCN_VF,
             COUNT(*) AS NBR_LCN_RD,
             0 AS NBR_LCN_IMP
    FROM
        r_ncrm_extraction.tmp_tbl_suivi_LCN_sgma v,
        r_ncrm_extraction.te_pcapt p
    WHERE
        1 = 1 and p.cdeBq="022"
           AND (bmlcn_rejet_delta=1)
		and v.BMTN_lecteur=p.cdePcapt
		and dateReception is null
        and concat(date,v.BMTN_lecteur)in ($P!{listparams})


        GROUP BY date , lecteur
    ORDER BY date , lecteur)
 UNION (
    -- REM & LCN IMPY
    SELECT
        v.date AS DATE,
            v.BMTN_lecteur AS LECTEUR,
             p.Lib AS LECTEUR_LIB,
			  p.circuit circuit,
			 0 AS NBR_LCN_DEC,
            0 AS NBR_LCN_TRAIT,
             0 AS NBR_LCN_SUPP,
			 0 AS NBR_LCN_VF,
            0 AS NBR_LCN_RD,
             COUNT(*) AS NBR_LCN_IMP

    FROM
        r_ncrm_extraction.tmp_tbl_suivi_LCN_sgma v,
        r_ncrm_extraction.te_pcapt p
    WHERE
        1 = 1 and p.cdeBq="022"
           AND (bmlcn_FLAG_IMP=1 )
		and v.BMTN_lecteur=p.cdePcapt
      and concat(date,v.BMTN_lecteur)in ($P!{listparams})


		and dateReception is null
        GROUP BY date , lecteur
    ORDER BY date , lecteur)

    ) AS RECAP
GROUP BY date , lecteur
ORDER BY date , lecteur]]>
	</queryString>
