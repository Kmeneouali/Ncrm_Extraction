	<queryString>
		<![CDATA[SELECT
    v.DATE,
    v.BMTN_lecteur,
    concat(v.BMTR_age , ' - ' , v.PCAPT_LIB) AS "Agence_emettrice",
    v.BMTR_CPT as 'compteBenificiaire',
    v.BMTR_NSER As 'NumRem',
    v.BMTR_MNT AS "Nnt_rem",
    v.BMCH_NSER AS "Num_Cheque",
    concat(v.banque,v.BMCH_LOC,"-",v.BMCH_CPT,v.BMCH_RIB) as 'cpt_chq',
    -- concat(v.Banque , ' - ' , v.Banque_Lib) AS "Banque_Destinataire",
    v.BMCH_MNT AS "Mnt",
    CASE BMTR_mode
       WHEN '1'THEN "Enc"
        WHEN '0' THEN "Esc"
        ELSE "-"
      END  AS  "type_Rem",
BMta_SQCA,BMtr_SQCA,BMch_SQCA
FROM etl.vw_traitement_aller_chq_bmci_etatproduction v
where  1=1
and v.bmch_ope_eta<> 'D'
and v.BMTR_ope_eta<> 'D'
and bmtr_mode='0'
 and   date=$P{dateInstance}
 and v.BMTN_lecteur=$P{cdepointCapture}

ORDER BY v.BMTR_age,BMta_SQCA,BMtr_SQCA,BMch_SQCA]]>
	</queryString>
