	<queryString>
		<![CDATA[SELECT
    DATE AS 'DATE',
    lecteur 'LECTEUR',
     LECTEUR_LIB AS 'LECTEUR_LIB',

    SUM(NBR_REM_DEC) AS 'NBR_REM_DEC',
    SUM(NBR_CHQ_DEC) AS 'NBR_CHQ_DEC',
    SUM(MNT_CHQ_DEC) AS 'MNT_CHQ_DEC',

    SUM(NBR_REM_TRAIT) AS 'NBR_REM_TRT',
    SUM(NBR_CHQ_TRAIT) AS 'NBR_CHQ_TRT',
    SUM(MNT_CHQ_TRAIT) AS 'MNT_CHQ_TRT',

	SUM(NBR_REM_SUPP) AS 'NBR_REM_SUPP',
    SUM(NBR_CHQ_SUPP) AS 'NBR_CHQ_SUPP',
    SUM(MNT_CHQ_SUPP) AS 'MNT_CHQ_SUPP',


    SUM(NBR_CHQ_VF) AS 'NBR_CHQ_VF',
    SUM(MNT_CHQ_VF) AS 'MNT_CHQ_VF',

    SUM(NBR_CHQ_RD) AS 'NBR_CHQ_RD',
    SUM(MNT_CHQ_RD) AS 'MNT_CHQ_RD'



    -- (SUM(NBR_REM_TRAIT) -SUM(NBR_REM_DEC) ) as DIFF_NBR_REM,
    -- (SUM(NBR_CHQ_TRAIT) -SUM(NBR_CHQ_DEC) ) as DIFF_NBR_CHQ,
    -- (SUM(MNT_CHQ_TRAIT) -SUM(MNT_CHQ_DEC) ) as DIFF_MNT
FROM
    (
    (-- REM & CHQ Déclarer
    SELECT
	 v.date AS 'DATE',
            v.BMTN_lecteur AS 'LECTEUR',
            p.Lib AS 'LECTEUR_LIB',

             COUNT(DISTINCT bmtr_nser)  AS 'NBR_REM_DEC',
            COUNT(*) AS 'NBR_CHQ_DEC',
            SUM(BMCH_MNT) AS 'MNT_CHQ_DEC',

            '0' AS 'NBR_REM_TRAIT',
            '0' AS 'NBR_CHQ_TRAIT',
            '0' AS 'MNT_CHQ_TRAIT',

             '0' AS 'NBR_REM_SUPP',
             '0' AS 'NBR_CHQ_SUPP',
             '0' AS 'MNT_CHQ_SUPP',


            '0' AS 'NBR_CHQ_VF',
            '0' AS 'MNT_CHQ_VF',

             '0' AS 'NBR_CHQ_RD',
            '0' AS 'MNT_CHQ_RD'
    FROM
        r_ncrm_extraction.tmp_tbl_eta_chq_sgda v
        ,r_ncrm_extraction.te_pcapt p
    WHERE
        1 = 1
         and STR_TO_DATE(date, '%Y-%m-%d')=$P{dateInstance}
           and v.BMTN_lecteur=p.cdePcapt
             and v.BMTN_lecteur=$P{cdepointCapture}
and p.cdeBq="363"
    GROUP BY date , lecteur
    ORDER BY date , lecteur)
    union


    (
    -- REM & CHQ Traitées
    SELECT
	  v.date AS 'DATE',
            v.BMTN_lecteur AS 'LECTEUR',
             p.Lib AS 'LECTEUR_LIB',

            '0' AS 'NBR_REM_DEC',
            '0' AS 'NBR_CHQ_DEC',
            '0' AS 'MNT_CHQ_DEC',

            COUNT(DISTINCT bmtr_nser) 'NBR_REM_TRAIT',
            COUNT(*) AS 'NBR_CHQ_TRAIT',
            SUM(BMCH_MNT) AS 'MNT_CHQ_TRAIT',

            '0' AS 'NBR_REM_SUPP',
             '0' AS 'NBR_CHQ_SUPP',
            '0' AS 'MNT_CHQ_SUPP',


            '0' AS 'NBR_CHQ_VF',
            '0' AS 'MNT_CHQ_VF',

             '0' AS 'NBR_CHQ_RD',
            '0' AS 'MNT_CHQ_RD'

    FROM
        r_ncrm_extraction.tmp_tbl_eta_chq_sgda v
        ,r_ncrm_extraction.te_pcapt p
    WHERE
        1 = 1
          and STR_TO_DATE(date, '%Y-%m-%d')=$P{dateInstance}
            AND BMCH_ope_eta <> 'D'
            AND BMTR_ope_eta <> 'D'
and bmch_CONF='1'
and p.cdeBq="363"
              and v.BMTN_lecteur=p.cdePcapt
              and v.BMTN_lecteur=$P{cdepointCapture}
    GROUP BY date , lecteur
    ORDER BY date , lecteur)
 UNION (
    -- REM & CHQ ANO
    SELECT
        v.date AS 'DATE',
            v.BMTN_lecteur AS 'LECTEUR',
             p.Lib AS 'LECTEUR_LIB',

            '0' AS 'NBR_REM_DEC',
            '0' AS 'NBR_CHQ_DEC',
            '0' AS 'MNT_CHQ_DEC',

           '0''NBR_REM_TRAIT',
            '0' AS 'NBR_CHQ_TRAIT',
            '0''MNT_CHQ_TRAIT',

             COUNT(DISTINCT bmtr_nser) as 'NBR_REM_SUPP',
             count(*) AS 'NBR_CHQ_SUPP',
             SUM(BMCH_MNT) AS 'MNT_CHQ_SUPP',


           '0' AS 'NBR_CHQ_VF',
            '0' AS 'MNT_CHQ_VF',

             '0' AS 'NBR_CHQ_RD',
            '0' AS 'MNT_CHQ_RD'
    FROM
        r_ncrm_extraction.tmp_tbl_eta_chq_sgda v
        ,r_ncrm_extraction.te_pcapt p
    WHERE
        1 = 1
          and STR_TO_DATE(date, '%Y-%m-%d')=$P{dateInstance}
           AND (BMCH_ope_eta='D'
           and  BMTR_ope_eta='D')
		and v.BMTN_lecteur=p.cdePcapt
        and v.BMTN_lecteur=$P{cdepointCapture}
and p.cdeBq="363"
        GROUP BY date , lecteur
    ORDER BY date , lecteur)
    UNION (
    -- REM & CHQ VF
    SELECT
        v.date AS 'DATE',
            v.BMTN_lecteur AS 'LECTEUR',
             p.Lib AS 'LECTEUR_LIB',

            '0' AS 'NBR_REM_DEC',
            '0' AS 'NBR_CHQ_DEC',
            '0' AS 'MNT_CHQ_DEC',

           '0''NBR_REM_TRAIT',
            '0' AS 'NBR_CHQ_TRAIT',
            '0''MNT_CHQ_TRAIT',

             '0' as 'NBR_REM_SUPP',
             '0' AS 'NBR_CHQ_SUPP',
             '0' AS 'MNT_CHQ_SUPP',


            COUNT(*) AS 'NBR_CHQ_VF',
            SUM(bmch_mnt) AS 'MNT_CHQ_VF',

             '0' AS 'NBR_CHQ_RD',
            '0' AS 'MNT_CHQ_RD'
    FROM
        r_ncrm_extraction.tmp_tbl_eta_chq_sgda v
        ,r_ncrm_extraction.te_pcapt p
    WHERE
        1 = 1
        and STR_TO_DATE(date, '%Y-%m-%d')=$P{dateInstance}
           AND (BMCH_CONF <> '1')
		and v.BMTN_lecteur=p.cdePcapt
        and v.BMTN_lecteur=$P{cdepointCapture}
and p.cdeBq="363"
        GROUP BY date , lecteur
    ORDER BY date , lecteur)
      UNION (
    -- REM & CHQ RDELTA
    SELECT
        v.date AS 'DATE',
            v.BMTN_lecteur AS 'LECTEUR',
             p.Lib AS 'LECTEUR_LIB',

            '0' AS 'NBR_REM_DEC',
            '0' AS 'NBR_CHQ_DEC',
            '0' AS 'MNT_CHQ_DEC',

           '0''NBR_REM_TRAIT',
            '0' AS 'NBR_CHQ_TRAIT',
            '0''MNT_CHQ_TRAIT',

             '0' as 'NBR_REM_SUPP',
             '0' AS 'NBR_CHQ_SUPP',
             '0' AS 'MNT_CHQ_SUPP',


            '0' AS 'NBR_CHQ_VF',
            '0' AS 'MNT_CHQ_VF',
             COUNT(*) AS 'NBR_CHQ_RD',
            SUM(bmch_mnt) AS 'MNT_CHQ_RD'
    FROM
        r_ncrm_extraction.tmp_tbl_eta_chq_sgda v
        ,r_ncrm_extraction.te_pcapt p
    WHERE
        1 = 1
        and STR_TO_DATE(date, '%Y-%m-%d')=$P{dateInstance}
           AND (bmch_rejet_delta='1')
		and v.BMTN_lecteur=p.cdePcapt
        and v.BMTN_lecteur=$P{cdepointCapture}
and p.cdeBq="363"
        GROUP BY date , lecteur
    ORDER BY date , lecteur)


    ) AS RECAP
GROUP BY date , lecteur
ORDER BY date , lecteur]]>
	</queryString>
