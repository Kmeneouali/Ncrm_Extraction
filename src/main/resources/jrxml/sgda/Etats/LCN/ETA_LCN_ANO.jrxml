	<queryString>
		<![CDATA[SELECT
    date,
    bmlcn_AGE_SCAN,
    BMTN_lecteur,
    BMTR_age,
    BMTR_CPT,
    BMTR_NSER AS TR_NSER,
    BMTR_MNT + 0 BMTR_MNT,
CASE bmtr_mode

        WHEN '1' THEN 'Encaissement'
        WHEN '2' THEN 'Escompte'
        WHEN '3' THEN 'Risque'
        ELSE '---'
	END  AS  'bmtr_mode' ,
    (SELECT
            SUM(bmlcn_mnt + 0)
        FROM
            r_ncrm_extraction.tmp_tbl_eta_lcn_sgma
        WHERE
            bmtr_nser = TR_NSER
        GROUP BY bmtr_nser
        ORDER BY BMTR_nser) sum_lcn,
    (BMTR_MNT - BMTN_sum_lcn) AS Diff,
    bmlcn_NSER,
    CONCAT(bmlcn_ZBK,
            bmlcn_LOC,
            bmlcn_CPT,
            bmlcn_RIB) AS cpt_tire,
    bmlcn_MNT + 0 bmlcn_MNT,
    bmlcn_SQCA,
   BMTN_ANO_LIB,
    BMTR_ANO_LIB,
    BMLCN_ANO_LIB
FROM
    r_ncrm_extraction.tmp_tbl_eta_lcn_sgma
WHERE
    (BMTN_ope_eta = 'D' OR BMTR_ope_eta = 'D' OR bmlcn_ope_eta = 'D')

and STR_TO_DATE(date, '%Y-%m-%d')=$P{dateInstance}
 and (BMTN_lecteur=$P{cdepointCapture})
and BMlcn_FLAG_SCAN_GR=$P{flag_scan_gr}
ORDER BY BMTR_age , BMTR_nser]]>
	</queryString>
